{
  "name": "baggie",
  "version": "0.0.1",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "bootstrap": "lerna bootstrap --no-ci --hoist",
    "build": "lerna run build",
    "test": "lerna run test --concurrency 1",
    "clean": "npx npm-run-all clean:*",
    "clean:lerna": "lerna clean --yes",
    "clean:lock": "rimraf **/package-lock.json",
    "clean:modules": "rimraf node_modules",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "lint:eslint": "eslint --cache --fix \"**/*.{js,jsx,ts,tsx}\"",
    "check-commit-msg": "node scripts/check-commit-msg",
    "deploy": "lerna publish --skip-npm && lerna run publish-npm"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:larsmunkholm/baggie.git"
  },
  "keywords": [
    "javascript-library",
    "library",
    "toolbox",
    "tools"
  ],
  "author": "Lars Munkholm <mail@larsersej.dk>",
  "license": "MIT",
  "homepage": "https://github.com/larsmunkholm/baggie#readme",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^13.13.15",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-tsdoc": "^0.2.7",
    "husky": "^4.2.5",
    "jest": "^26.6.1",
    "lerna": "^3.22.1",
    "lint-staged": "^10.2.11",
    "prettier": "2.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "npm run check-commit-msg"
    }
  }
}
